# platform :ios, '9.0'

def shared_pods
  pod 'MusicTheorySwift'
  pod 'AudioKit'
  pod 'Combinatorics'
end

target 'MIDISequencer iOS' do
  use_frameworks!
  shared_pods
end

target 'MIDISequencer Mac' do
  use_frameworks!
  shared_pods
end
